<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		id="valueHelpDialog"
		title="Books"
		items="{LibraryData>/customer}"
		search="_handleCustValueHelpSearch"
		confirm="_handleCustValueHelpClose"
		cancel="_handleCustValueHelpClose"
		>
		<StandardListItem
			title="{LibraryData>Name}"
			description="{LibraryData>Email}" />
	</SelectDialog>
</core:FragmentDefinition>